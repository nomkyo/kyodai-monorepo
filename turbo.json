{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": ["VSCODE_INSPECTOR_OPTIONS", "NODE_ENV"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "lint": {
      "dependsOn": ["^lint"],
      "dotEnv": ["apps/backend/.env.example"]
    },
    "test": {
      "dependsOn": ["env"]
    },
    "test:e2e": {
      "dependsOn": ["test:e2e"]
    },
    "env": {},
    "dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["env"]
    },
    "format": {}
  }
}
