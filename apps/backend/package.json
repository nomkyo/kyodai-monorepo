{
  "name": "backend",
  "version": "0.0.1",
  "description": "Backend for Kyodai",
  "author": "Nomura Kyodai",
  "license": "MIT",
  "keywords": [
    "nestjs",
    "prisma",
    "prisma client",
    "typescript",
    "passport",
    "graphql"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/fivethree-team/nestjs-prisma-client-example.git"
  },
  "bugs": {
    "url": "https://github.com/fivethree-team/nestjs-prisma-client-example/issues"
  },
  "scripts": {
    "build": "nest build --builder webpack",
    "env": "cp -u .env.example .env",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "pnpm supabase:reset && nest start --watch",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_ENV=prod node dist/main",
    "lint": "dotenv -e ./.env -- eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "migrate:dev": "prisma migrate dev --preview-feature",
    "migrate:dev:create": "prisma migrate dev --create-only --preview-feature",
    "migrate:reset": "prisma migrate reset --preview-feature",
    "migrate:deploy": "pnpm exec prisma migrate deploy --preview-feature",
    "migrate:status": "pnpm exec prisma migrate status --preview-feature",
    "migrate:resolve": "pnpm exec prisma migrate resolve --preview-feature",
    "prisma:studio": "pnpm exec prisma studio",
    "prisma:generate": "pnpm exec prisma generate",
    "prisma:generate:watch": "pnpm exec prisma generate --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "start:db": "pnpm run migrate:up && pnpm run prisma:generate && pnpm run seed",
    "seed": "pnpm exec prisma db seed",
    "supabase:start": "supabase start -x realtime,storage-api,imgproxy,postgres-meta,edge-runtime,logflare,vector,supavisor",
    "supabase:reset": "supabase stop && pnpm supabase:start",
    "docker:migrate": "docker-compose -f docker-compose.migrate.yml up -d",
    "docker:db": "docker-compose -f docker-compose.db.yml up -d",
    "docker:db:reset": "docker-compose down && sudo kill -9 `sudo lsof -t -i:5432` ; pnpm docker:db && pnpm seed",
    "docker:seed": "docker exec -it nest-api pnpm run seed",
    "docker": "docker-compose up -d",
    "docker:build": "docker-compose build"
  },
  "dependencies": {
    "@apollo/server": "^4.7.5",
    "@devoxa/prisma-relay-cursor-connection": "2.2.3",
    "@nestjs/apollo": "12.0.7",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "10.1.0",
    "@nestjs/config": "3.0.0",
    "@nestjs/core": "10.1.0",
    "@nestjs/graphql": "12.0.8",
    "@nestjs/jwt": "10.1.0",
    "@nestjs/passport": "10.0.0",
    "@nestjs/platform-express": "10.1.0",
    "@nestjs/swagger": "7.1.2",
    "@prisma/client": "^5.0.0",
    "@supabase/ssr": "^0.4.0",
    "@supabase/supabase-js": "^2.45.0",
    "axios": "^1.6.8",
    "bcrypt": "5.1.0",
    "cache-manager": "^5.5.2",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.5",
    "graphql": "16.7.1",
    "graphql-scalars": "1.22.2",
    "graphql-subscriptions": "2.0.0",
    "lodash": "^4.17.21",
    "nestjs-prisma": "0.22.0-dev.0",
    "passport": "0.6.0",
    "passport-jwt": "4.0.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "10.1.10",
    "@nestjs/schedule": "^4.0.2",
    "@nestjs/schematics": "10.0.1",
    "@nestjs/testing": "10.1.0",
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.5.5",
    "@types/bcrypt": "5.0.0",
    "@types/chance": "1.1.3",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "4.17.17",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.3.1",
    "@types/supertest": "2.0.12",
    "@types/ws": "^8.5.12",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "@vitest/coverage-v8": "0.33.0",
    "chance": "1.1.11",
    "cheerio": "1.0.0-rc.12",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "29.6.1",
    "nock": "^13.5.4",
    "prettier": "2.8.8",
    "prisma": "5.0.0",
    "prisma-dbml-generator": "^0.12.0",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "9.4.4",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.1.6",
    "unplugin-swc": "^1.4.5",
    "vitest": "1.6.0",
    "ws": "^8.18.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "@App/(.*)": "<rootDir>/src/$1"
    },
    "rootDir": "test",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "setupFiles": [
      "./jestSetup.ts"
    ]
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
