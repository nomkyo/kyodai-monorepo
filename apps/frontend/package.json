{
	"name": "frontend",
	"author": "Nom Kyodai <nomurakyodai@gmail.com>",
	"description": "Nomura Kyodai Frontend",
	"repository": {
		"type": "git",
		"url": "https://github.com/nomkyo/kyodai-monorepo"
	},
	"private": true,
	"license": "MIT",
	"version": "1.0.0",
	"scripts": {
		"env": "cp -u .env.example .env",
		"setup": "git init && husky install && npx playwright install",
		"format": "prettier src/**/*.{ts,tsx} --write",
		"lint": "eslint \"{src,apps,libs,test}/**/*.{ts,tsx}\"  --report-unused-disable-directives --max-warnings 0",
		"lint:fix": "eslint \"{src,apps,libs,test}/**/*.{ts,tsx}\"  --report-unused-disable-directives --max-warnings 0 --fix",
		"dev": "vite",
		"storybook": "storybook dev -p 6006",
		"storybook:setup": "node node_modules/@storybook/addon-styling/bin/postinstall.js",
		"storybook:build": "storybook build",
		"test": "vitest run src/",
		"test:unit": "vitest src/",
		"test:unit:coverage": "vitest --coverage src/",
		"test:e2e:staging": "NODE_ENV=staging playwright test",
		"test:e2e:dev": "NODE_ENV=dev playwright test",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"commitlint": "commitlint --edit",
		"commitizen": "exec < /dev/tty && git cz --hook || true"
	},
	"dependencies": {
		"@emotion/react": "^11.11.4",
		"@hookform/resolvers": "^3.3.4",
		"@mantine/core": "^7.9.1",
